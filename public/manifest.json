{
	"name": "Domain lookup",
	"description": "Check for whois and server information of a domain",
	"version": "1.0.0",
	"author": "Bhuvan Gandhi",
	"manifest_version": 3,
	"icons": {
		"16": "logo_16x16.png",
		"48": "logo_48x48.png",
		"128": "logo_128x128.png"
	},
	"action": {},
	"background": {
		"service_worker": "./bundle/worker.js",
		"type": "module"
	},
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"./bundle/script.js"
			],
			"run_at": "document_end"
		}
	],
	"web_accessible_resources": [
		{
			"resources": [
				"/assets/logo.jpg",
				"/bundle/main.css"
			],
			"matches": [
				"<all_urls>"
			]
		}
	]
}